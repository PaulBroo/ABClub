<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pagamentos.MenuPagamentos">

    <Button
        android:id="@+id/pagamento"
        android:layout_width="157dp"
        android:layout_height="50dp"
        android:layout_marginBottom="428dp"
        android:text="Pagamentos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Txtquotas" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="229dp"
        android:layout_height="150dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="82dp"
        android:layout_marginBottom="35dp"
        android:text="Metodos de Pagamento"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="50dp"
        android:layout_height="150dp"
        android:layout_marginStart="33dp"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="50dp"
        android:text="Total de quotas a pagar"
        app:layout_constraintBottom_toTopOf="@+id/Txtquotas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/Txtquotas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="133dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/pagamento"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />


</androidx.constraintlayout.widget.ConstraintLayout>